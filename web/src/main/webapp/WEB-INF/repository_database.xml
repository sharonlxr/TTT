	username="tttfinal" password="tttfinal">
	<sequence-manager
		className="org.apache.ojb.broker.util.sequence.SequenceManagerNextValImpl">
		<attribute attribute-name="autoNaming" attribute-value="true" />
	</sequence-manager>
</jdbc-connection-descriptor>
